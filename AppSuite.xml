<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="AppSuite" parallel="tests" thread-count="5">
	<test parallel="classes" name="Test">
		<!-- Defining a parameter in the XML -->
		<parameter name="platform" value="web" />
		<parameter name="browser" value="Chrome" />
		<classes>
			<class name="appTests.ChangeSetting" />
			<class name="appTests.SearchFlow" />
			<class name="appTests.TestAppFlow" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->
